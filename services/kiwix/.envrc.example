#!/bin/sh
source ../../helpers/bitwarden.sh

export ANSIBLE_HOST="$(get_custom_field 'enkidu' 'SERVER_IP')"
export ANSIBLE_PORT="$(get_custom_field 'enkidu' 'SERVER_PORT')"
export ANSIBLE_USER="$(get_username 'enkidu')"
export ANSIBLE_SUDO_PASS="$(get_password 'enkidu')"

export DOMAIN="$(get_custom_field 'k8s - kiwix' 'DOMAIN')"
export UPTIME_ENDPOINT=$(get_custom_field 'k8s - kiwix' 'UPTIME_ENDPOINT')
export IP_WHITELIST=$(get_custom_field 'k8s - kiwix' 'IP_WHITELIST')
export KIWIX_PATH=$(get_custom_field 'k8s - kiwix' 'KIWIX_PATH')
